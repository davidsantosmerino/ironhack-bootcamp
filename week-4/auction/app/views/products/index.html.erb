<h2>Products</h2>
<ul>
  <%@products.each do |product|%>
    <li>
      <span>
        <a href="/users/<%=@user.id%>/products/<%=product.id%>">
          <%=product.title%>
        </a>
        <small>
          <%=product.deadline%>
        </small>
      </span>
    </li>
  <%end%>
</ul>
<a href="/users/<%=@user.id%>/products/new">add product</a>
